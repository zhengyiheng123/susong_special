<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingRight="2dp"

    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#fff"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <ImageView
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@mipmap/wodedingdan" />

        <TextView
            android:id="@+id/pop_tv_order"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center_vertical"
            android:paddingLeft="12dp"

            android:text="我的订单        "
            android:textColor="@color/material_textBlack_black"
            android:textSize="@dimen/text_14" />
    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/line_height"
        android:background="@color/line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#fff"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <ImageView
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@mipmap/tichengdingdan" />

        <TextView
            android:id="@+id/pop_tv_commission_order"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center_vertical"
            android:paddingLeft="12dp"
            android:text="提成订单"
            android:textColor="@color/material_textBlack_black"
            android:textSize="@dimen/text_14" />
    </LinearLayout>



    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/line_height"
        android:background="@color/line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#fff"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <ImageView
            android:layout_width="16dp"
            android:layout_height="16dp"

            android:src="@mipmap/paihangbang" />

        <TextView
            android:id="@+id/pop_tv_rank"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center_vertical"
            android:paddingLeft="12dp"
            android:text="排行榜"
            android:textColor="@color/material_textBlack_black"
            android:textSize="@dimen/text_14" />
    </LinearLayout>



    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/line_height"
        android:background="@color/line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#fff"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <ImageView
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@mipmap/wodeshoucang" />

        <TextView
            android:id="@+id/pop_tv_collection"
            android:layout_width="match_parent"
            android:layout_height="40dp"

            android:gravity="center_vertical"
            android:paddingLeft="12dp"
            android:text="我的收藏"
            android:textColor="@color/material_textBlack_black"
            android:textSize="@dimen/text_14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/line_height"
        android:background="@color/line" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:background="#fff"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <ImageView
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@mipmap/erweima111" />

        <TextView
            android:id="@+id/pop_tv_code"
            android:layout_width="match_parent"
            android:layout_height="40dp"

            android:gravity="center_vertical"
            android:paddingLeft="12dp"
            android:text="我的二维码"
            android:textColor="@color/material_textBlack_black"
            android:textSize="@dimen/text_14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/line_height"
        android:background="@color/line" />

    <RelativeLayout
        android:id="@+id/mine_tv_message"
        android:layout_width="match_parent"
        android:background="#fff"
        android:paddingLeft="20dp"
        android:layout_height="40dp">

        <ImageView
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:id="@+id/wodeshoucang"
            android:layout_centerVertical="true"
            android:src="@mipmap/wodexiaoxi" />
        <TextView
            android:id="@+id/pop_tv_messages"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:paddingLeft="@dimen/margin_12"
            android:layout_toRightOf="@id/wodeshoucang"
            android:gravity="center_vertical"
            android:text="我的消息"
            android:textColor="@color/material_textBlack_black"
            android:textSize="@dimen/text_14" />

        <View
            android:id="@+id/pop_view_circle"
            android:layout_width="6dp"
            android:layout_height="6dp"
            android:layout_marginTop="12dp"
            android:layout_marginLeft="4dp"
            android:layout_toRightOf="@id/pop_tv_messages"
            android:background="@drawable/shape_circle_red"
            android:visibility="gone" />


    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/line_height"
        android:background="@color/line" />

